@import "../../../common/wxss/buildSearch.wxss";
@import "../../../common/wxss/comWxss.wxss";
.list-wrap {
	margin-top: 100rpx;
}
.list-div {
	width: 100%;
	height: 150rpx;
	border-bottom: 1rpx solid #DCDCDC;
}
.list-data {
	width: 90%;
	margin: 0 auto;
}
.list-data .v1 {
	width: 30%;
	height: 150rpx;
}
.list-data .v1 .t1 {
	font-size: 29rpx;
	margin-top: 40rpx;
}
.list-data .v1 .t2 {
	color: #929292;
	font-size: 52%;
}
.list-data .v2 {
	width: 49%;
	margin-top: 26rpx;
}
.list-data .v2 image{
	width: 100rpx;
	height: 100rpx;
	margin-left: 7rpx;
}
.list-data .v3 {
	width: 20%;
	margin-top: 50rpx;
}
.list-data .v3 image {
	width: 51rpx;
	height: 52rpx;
	margin-left: 16rpx;
}
.view-pop-wrap {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0,0,0,.6);
}
.view-pop-img {
  position: fixed;
  z-index: 5000;
  width: 100%;
  height: 550rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
  border-radius: 3px;
  overflow: hidden;
}
.view-pop-img image {
  width: 100%;
  height: 100%;
}
.loading-data {
	margin: 20rpx 0 60rpx 0;
	color: #A09898;
	text-align: center;
	font-size: 30rpx;
}