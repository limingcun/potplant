@import "../../../common/wxss/hpic.wxss";
@import "../../../common/wxss/comWxss.wxss";
.list-head {
	width: 100%;
	height: 1rpx;
	background-color: #DCDCDC;
}
.list-div {
	width: 100%;
	height: 150rpx;
	border-bottom: 1rpx solid #DCDCDC;
}
.list-data {
	width: 90%;
	margin: 0 auto;
}
.list-data .v1 {
	width: 30%;
	height: 150rpx;
	margin-left: 25rpx;
}
.list-data .v1 .t1 {
	font-size: 29rpx;
	margin-top: 40rpx;
}
.list-data .v1 .t2 {
	color: #929292;
	font-size: 52%;
}
.list-data .v3 {
	width: 20%;
	margin-top: 50rpx;
}
.list-data .v3 image {
	width: 51rpx;
	height: 52rpx;
	margin-left: 16rpx;
}
.loading-data {
	margin: 20rpx 0 60rpx 0;
	color: #A09898;
	text-align: center;
	font-size: 30rpx;
}